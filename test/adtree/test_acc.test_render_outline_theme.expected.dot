graph {
	graph [nodesep=0.2 overlap=False ranksep=0.4 splines=True]
	R [label="the goal" color="#ff5c5c" fillcolor="#ffffff" fontname=Arial margin=0.1 shape=ellipse style="setlinewidth(3)"]
	"R.0" [label=path1 color="#ff5c5c" fillcolor="#ffffff" fontname=Arial margin=0.1 shape=ellipse style="setlinewidth(3)"]
	"R.0.0" [label="defend path1" color="#27B011" fillcolor="#ffffff" fontname=Arial margin=0.1 shape=box style="setlinewidth(3)"]
	"R.0.0.0" [label="path1 defence defeated" color="#ff5c5c" fillcolor="#ffffff" fontname=Arial margin=0.1 shape=ellipse style="setlinewidth(3)"]
	"R.0.0" -- "R.0.0.0" [color="#1f1f1f" fontname=Arial style=dashed]
	"R.0" -- "R.0.0" [color="#1f1f1f" fontname=Arial style=dashed]
	R -- "R.0" [color="#1f1f1f" fontname=Arial style=solid]
	"R.1" [label=path2 color="#ff5c5c" fillcolor="#ffffff" fontname=Arial margin=0.1 shape=ellipse style="setlinewidth(3)"]
	"R.1.0" [label="path2.1" color="#ff5c5c" fillcolor="#ffffff" fontname=Arial margin=0.1 shape=ellipse style="setlinewidth(3)"]
	"R.1" -- "R.1.0" [color="#1f1f1f" fontname=Arial style=solid]
	"R.1.1" [label=AND color="#ff5c5c" fillcolor="#ff5c5c" fontname=Arial margin=0.05 shape=triangle style="rounded, setlinewidth(3)"]
	"R.1.1.0" [label="path3.1" color="#ff5c5c" fillcolor="#ffffff" fontname=Arial margin=0.1 shape=ellipse style="setlinewidth(3)"]
	"R.1.1" -- "R.1.1.0" [color="#1f1f1f" fontname=Arial style=solid]
	"R.1.1.1" [label="path3.2" color="#ff5c5c" fillcolor="#ffffff" fontname=Arial margin=0.1 shape=ellipse style="setlinewidth(3)"]
	"R.1.1" -- "R.1.1.1" [color="#1f1f1f" fontname=Arial style=solid]
	"R.1" -- "R.1.1" [color="#1f1f1f" fontname=Arial style=solid]
	R -- "R.1" [color="#1f1f1f" fontname=Arial style=solid]
}
